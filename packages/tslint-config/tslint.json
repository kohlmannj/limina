{
  "extends": ["tslint:latest", "tslint-eslint-rules", "tslint-config-prettier"],
  "rules": {
    "no-implicit-dependencies": [true, "dev"],
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case",
      "ban-keywords",
      "check-format"
    ]
  }
}
