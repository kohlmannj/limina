{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": {
      "severity": "error",
      "options": {
        "bracketSpacing": true,
        "printWidth": 100,
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "es5"
      }
    }
  }
}
